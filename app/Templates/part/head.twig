<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta http-equiv="X-UA-Compatible" content="ie=edge">
<link href="{{asset("components/data.css")}}" rel="stylesheet">
<link href="https://fonts.googleapis.com/css?family=Oxygen|Montserrat|Material+Icons|Source+Sans+Pro|Open+Sans:300,400,700&subset=latin-ext" rel="stylesheet">
<link rel="shortcut icon" type="image/x-icon" href="{{asset("img/ui/fav.png")}}">
{% if fulltitle is defined %}
    {% set ctitle = fulltitle %}
{% else %}
    {% set ctitle = title is defined?title~' | '~app['conf.title']:app['conf.title']%}
{% endif %}
<title>{{ctitle}}</title>
{% if meta is defined %}
    {% if meta.meta is defined %}
        {% for key,itm in meta.meta %}
        <meta name="{{key}}" content="{{itm}}">
        {% endfor %}
    {% endif %}
    {% if meta.og is defined %}
        {% for key,itm in meta.og %}
        <meta property="og:{{key}}" content="{{itm}}">
        {% endfor %}
    {% endif %}
{% endif %}
